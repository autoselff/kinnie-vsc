{
	"name": "kinnie",
	"scopeName": "source.kinnie",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#identifiers"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.double-slash.kinnie",
					"match": "//.*$"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.kinnie",
					"begin": "\"",
					"end": "\""
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.kinnie",
					"match": "\\b\\d+\\b"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.kinnie",
					"match": "\\b(var|create|if|else|repeat|fun)\\b"
				},
				{
					"name": "keyword.other.print.kinnie",
					"match": "\\b(print|printl)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.comparison.kinnie",
					"match": "==|!=|>=|<=|>|<|%"
				},
				{
					"name": "keyword.operator.assignment.kinnie",
					"match": "="
				},
				{
					"name": "keyword.operator.arithmetic.kinnie",
					"match": "\\+|-|\\*|/"
				},
				{
					"name": "punctuation.section.block.kinnie",
					"match": "[{}]"
				},
				{
					"name": "punctuation.section.parenthesis.kinnie",
					"match": "\\(\\)"
				}
			]
		},
		"identifiers": {
			"patterns": [
				{
					"name": "variable.other.kinnie",
					"match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
				}
			]
		}
	}
}